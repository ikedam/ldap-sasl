<?xml version='1.0' encoding='UTF-8'?>
<securityRealm class="jp.ikedam.jenkins.plugins.ldap_sasl.LdapSaslSecurityRealm" plugin="ldap-sasl@0.1.0">
  <ldapUriList>
    <string>ldap://127.0.0.1:8389/</string>
  </ldapUriList>
  <mechanismList class="java.util.Arrays$ArrayList">
    <a class="string-array">
      <string>DIGEST-MD5</string>
    </a>
  </mechanismList>
  <groupSearchBase>dc=example,dc=com</groupSearchBase>
  <groupPrefix>ROLE_</groupPrefix>
  <connectionTimeout>5000</connectionTimeout>
  <readTimeout>3000</readTimeout>
</securityRealm>
